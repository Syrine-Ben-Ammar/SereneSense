# SereneSense environment configuration

# API configuration
SERENESENSE_API_PORT=8000
SERENESENSE_ENV=development

# Model and inference configuration
SERENESENSE_MODEL_PATH=/workspace/models/audioMAE_latest.pth
SERENESENSE_MODEL_TYPE=audioMAE
SERENESENSE_INFERENCE_CONFIG=configs/deployment/cloud.yaml
SERENESENSE_INFERENCE_DATASET=mad
SERENESENSE_INFERENCE_SPLIT=validation

# Training configuration
SERENESENSE_TRAIN_CONFIG=configs/training/audioMAE.yaml
SERENESENSE_TRAIN_DATASET=mad
SERENESENSE_TRAIN_OUTPUT_DIR=/workspace/outputs

# Monitoring and experiment tracking
SERENESENSE_TENSORBOARD_PORT=6006
SERENESENSE_MLFLOW_PORT=5000
SERENESENSE_MLFLOW_URI=http://mlflow:5000
SERENESENSE_MLFLOW_BACKEND_URI=sqlite:///mlflow.db
SERENESENSE_MLFLOW_ARTIFACT_ROOT=/mlruns

# Container registry overrides (used in docker-compose.prod.yml)
SERENESENSE_REGISTRY=ghcr.io/serenesense
SERENESENSE_VERSION=latest

# Dataset directories mounted into containers
SERENESENSE_DATA_DIR=/workspace/data
SERENESENSE_MODEL_DIR=/workspace/models
SERENESENSE_LOG_DIR=/workspace/logs
